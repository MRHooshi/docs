                    ],
                    "httpRequest": {
                        "method": "POST",
                        "headerAttributes": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            },
                            {
                                "key": "Authorization",
                                "value": "Token TOKEN_KEY"
                            }
                        ],
                        "sections": [
                            {
                                "type": "messageBody",
                                "contentType": "application/json",
                                "content": "{\n    \"image_url\": \"http://www.sobhe.ir/baaz/img/goshawk.jpg\"\n}\n"
                            }
                        ]
                    },
                    "httpResponse": {
                        "headerAttributes": [],
                        "sections": [
                            {
                                "type": "messageBody",
                                "content": "{\n    \"image_url\": \"http://www.sobhe.ir/baaz/img/goshawk.jpg\",\n    \"removed_reports\": [1]\n}\n"
                            }
                        ]
                    }
                }
            ]
